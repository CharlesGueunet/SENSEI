#ifndef svtk_jsoncpp_h
#define svtk_jsoncpp_h

/* Use the jsoncpp library configured for SVTK.  */
#cmakedefine01 SVTK_MODULE_USE_EXTERNAL_svtkjsoncpp

#cmakedefine01 SVTK_JSONCPP_SHARED

#if SVTK_MODULE_USE_EXTERNAL_svtkjsoncpp
