# endif
# include <json/json.h>
#else
// Needed for Windows declspec import logic
# if !defined(JSON_DLL) && defined(_WIN32) && SVTK_JSONCPP_SHARED
#  define JSON_DLL
# endif
# include <svtkjsoncpp/json/json.h>
#endif

#endif
