#ifndef svtk_libproj_h
#define svtk_libproj_h

/* Use the libproj library configured for SVTK.  */
#cmakedefine01 SVTK_MODULE_USE_EXTERNAL_svtklibproj

#define SVTK_LibPROJ_MAJOR_VERSION @LibPROJ_MAJOR_VERSION@

#if SVTK_LibPROJ_MAJOR_VERSION < 5
# define PROJ_VERSION_MAJOR @LibPROJ_MAJOR_VERSION@
# define proj_list_operations pj_get_list_ref
#endif

#if SVTK_MODULE_USE_EXTERNAL_svtklibproj
# if SVTK_LibPROJ_MAJOR_VERSION >= 5
#  include <proj.h>
# endif
# if SVTK_LibPROJ_MAJOR_VERSION < 6
#  include <projects.h>
# endif
# if SVTK_LibPROJ_MAJOR_VERSION >= 6
#  define ACCEPT_USE_OF_DEPRECATED_PROJ_API_H 1
# endif
# include <proj_api.h>
# include <geodesic.h>
#else
# include <svtklibproj/src/projects.h>
# include <svtklibproj/src/proj_api.h>
# include <svtklibproj/src/geodesic.h>
#endif

#endif
